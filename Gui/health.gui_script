function on_message(self, message_id, message, sender)
	if message_id == hash("updatenode") then
		if message.position then gui.set_position(gui.get_node("background"), message.position); gui.set_position(gui.get_node("health"), message.position) end
		print("health_changed")
		gui.set_fill_angle(gui.get_node("health"), 360/message.health)
	end
end
